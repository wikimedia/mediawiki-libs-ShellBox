	"jsonLogToStderr": false,
	"syslogIdent": "shellbox",
	"logToSyslog": false,
	"logToClient": true,
	"allowedActions": [ "call", "shell" ],
	"useSystemd": null,
	"useBashWrapper": null,
	"useFirejail": null,
	"firejailPath": "/usr/bin/firejail"
}
